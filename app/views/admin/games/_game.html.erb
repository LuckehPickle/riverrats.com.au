<tr>
  <td class="stretch">
    <%= link_to edit_admin_game_path(game), remote: true, data: { expects_modal: true, disable: true } do %>
      <span class="highlight">Game <%= game.name %></span> at the <%= game.venue.name %>
    <%- end %>
  </td>
  <td><%= local_time_ago game.played_on %></td>
</tr>